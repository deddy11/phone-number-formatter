{
  "name": "phone-number-formatter",
  "displayName": "Phone Number Formatter",
  "description": "Format phone numbers in a consistent way",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "phone-number-formatter.format",
        "title": "Format Phone Number"
      },
      {
        "command": "phone-number-formatter.clean",
        "title": "Clean Phone Numbers"
      },
      {
        "command": "phone-number-formatter.generateNames",
        "title": "Generate List of Names"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
